<?xml version='1.0' encoding='utf-8'?>
<widget
  id="ar.com.satyam.roxymusic"
  version="1.0.0"
  xmlns="http://www.w3.org/ns/widgets"
  xmlns:gap="http://phonegap.com/ns/1.0"
  >
    <name>Roxy Music</name>
    <description>
        Roxy media player.
    </description>
    <author email="satyam@satyam.com.ar" href="http://satyam.com.ar">
        Daniel Barreiro
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-file" source="npm" spec="~4.1.1" />
    <plugin name="cordova-plugin-file-transfer" source="npm" spec="~1.5.0" />
    <plugin name="cordova-plugin-media" source="npm" spec="~2.2.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <icon src="icon.png" />
    <platform name="android">
        <icon quality="ldpi" src="www/res/icon/android/drawable-ldpi-icon.png" />
        <icon quality="mdpi" src="www/res/icon/android/drawable-mdpi-icon.png" />
        <icon quality="hdpi" src="www/res/icon/android/drawable-hdpi-icon.png" />
        <icon quality="xhdpi" src="www/res/icon/android/drawable-xhdpi-icon.png" />
        <icon quality="xxhdpi" src="www/res/icon/android/drawable-xxhdpi-icon.png" />
        <icon quality="xxxhdpi" src="www/res/icon/android/drawable-xxxhdpi-icon.png" />
        <splash quality="land-ldpi" src="www/res/screen/android/drawable-land-ldpi-screen.png" />
        <splash quality="land-mdpi" src="www/res/screen/android/drawable-land-mdpi-screen.png" />
        <splash quality="land-hdpi" src="www/res/screen/android/drawable-land-hdpi-screen.png" />
        <splash quality="land-xhdpi" src="www/res/screen/android/drawable-land-xhdpi-screen.png" />
        <splash quality="land-xxhdpi" src="www/res/screen/android/drawable-land-xxhdpi-screen.png" />
        <splash quality="land-xxxhdpi" src="www/res/screen/android/drawable-land-xxxhdpi-screen.png" />
        <splash quality="port-ldpi" src="www/res/screen/android/drawable-port-ldpi-screen.png" />
        <splash quality="port-mdpi" src="www/res/screen/android/drawable-port-mdpi-screen.png" />
        <splash quality="port-hdpi" src="www/res/screen/android/drawable-port-hdpi-screen.png" />
        <splash quality="port-xhdpi" src="www/res/screen/android/drawable-port-xhdpi-screen.png" />
        <splash quality="port-xxhdpi" src="www/res/screen/android/drawable-port-xxhdpi-screen.png" />
        <splash quality="port-xxxhdpi" src="www/res/screen/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
